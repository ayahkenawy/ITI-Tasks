@{
    int ssn = 10;
    string fname = "Aya";
    string lname = "Ahmed";
    int count = 0;
    List<string> names = new List<string>() {"Alaa","Omnya","Ahmed","Aya","Khaled" };
    List<TryViews.Models.Employee> employess = new List<TryViews.Models.Employee>()
      {
          new TryViews.Models.Employee{Id=1,Name="Aya",Age=30,Salary=5000},
          new TryViews.Models.Employee{Id=2,Name="Khaled",Age=35,Salary=10000},
          new TryViews.Models.Employee{Id=3,Name="Omnya",Age=29,Salary=4000},
          new TryViews.Models.Employee{Id=4,Name="Alaa",Age=24,Salary=3000},
          new TryViews.Models.Employee{Id=5,Name="Muhamed",Age=26,Salary=5000},
          new TryViews.Models.Employee   {Id=6,Name="Ahmed",Age=50,Salary=10000},
      };
}
<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Index</title>
</head>
<body>
    <table border="3">
        <tr>
            <th>ID</th>
            <th>Name</th>
            <th>Age</th>
            <th>Salary</th>
        </tr>
        @foreach (var item in employess)
        {
            <tr>
                <td>@item.Id</td>
                <td>@item.Name</td>
                <td>@item.Age</td>
                <td>@item.Salary</td>
            </tr>
        }
    </table>
    <table border="3">
        <tr>
            <th>Number</th>
            <th>Names</th>
        </tr>
       
            @foreach (var item in names)
        {
            <tr>
                @{count++;}
                <td>@count</td>
                <td>@item</td>
            </tr>
           }
       
    </table>
    @if (ssn>10)
    {
        <h1 style="color:aquamarine;">@($"{fname} {lname}")</h1>
    }
    else
    {
        <h1 style="color:red;">@($"{fname} {lname}")</h1>
    }
    <h1 style="color:aquamarine;">@ssn</h1>
    <h1 style="color:aquamarine;">@fname</h1>
    <h1 style="color:aquamarine;">@lname</h1>
</body>
</html>
